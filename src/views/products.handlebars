  {{log products}}

  <h1>Mis productos</h1>
  <div class="container">
    <div class="cardContainerHome">
      {{#each products}}
      {{log this}}
        <div class="card homeCard">
          <div><b>Codigo de producto</b>: {{this.code}}</div><div><b>Producto:</b>
            {{this.title}}</div><div><b>Descripci√≥n:</b>
